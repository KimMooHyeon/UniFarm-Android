<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.song2.unifarm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:name=".Network.ApplicationController"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@drawable/unifarm"
            android:supportsRtl="true"
            android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".ThanksForReviewActivity"
                android:theme="@style/categoryEditTheme"></activity>

        <uses-library
                android:name="org.apache.http.legacy"
                android:required="false" />
        <uses-library
                android:name="com.google.android.maps"
                android:required="true" />

        <meta-data
                android:name="com.google.android.geo.API_KEY"
                android:value="AIzaSyBBgJZ8jrEbMp1XfKCc0Mzy2XGJ3bfopFo" />

        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUp2Activity" />
        <activity android:name=".SignUp1Activity" />
        <activity android:name=".LoginActivity" /> <!-- 메인 -->
        <activity android:name=".MainActivity" />
        <activity android:name=".DetailedActivity"></activity>
        <activity android:name=".SearchActivity"></activity> <!-- 유니팜 모아보기 -->
        <activity android:name=".CollectViewActivity" /> <!-- 캘린더 -->
        <activity android:name=".KotlinCalendar" />
        <activity
                android:name=".ReviewActivity"
                android:theme="@style/categoryEditTheme" /> <!-- 설정 -->
        <activity android:name=".SettingActivity" /> <!-- 리워드 -->
        <activity android:name=".RewordActivity"></activity>
    </application>

</manifest>